main() {
	int i;
	int n;
	int n_1;
	int n_2;

	i = 2;
	n_1 = 1;
	n_2 = 0;

	prints("fib(0) = 0\n");
	prints("fib(1) = 1\n");

	while (i < 47) {
		prints("fib(");
		printi(i);
		prints(") = ");

		n = n_1 + n_2;
		printi(n);
		n_2 = n_1;
		n_1 = n;

		prints("\n");
		i = i + 1;
	}
}
